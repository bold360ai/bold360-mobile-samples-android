{
  "name": "BoldCore",
  "version": "1.0.x-dev",
  "summary": "BoldCore is an SDK.",
  "description": "BoldCore is an SDK.",
  "homepage": "https://github.com/nanorepsdk/NanorepUI/wiki",
  "license": "private",
  "authors": "bold360ai",
  "source": {
    "git": "https://bitbucket.3amlabs.net/scm/bold/boldcore-mobile-ios.git",
    "tag": "1.0.x-dev"
  },
  "public_header_files": "BoldCore/**/*.h",
  "source_files": [
    "BoldCore/**/*.{h,m,swift,js,json,xcassets}"
  ],
  "resources": [
    "BoldCore/**/*.{xib, png, jpeg, jpg, js}"
  ],
  "resource_bundles": {
    "BoldCore": [
      "BoldCore/**/*.{xcassets,js}"
    ],
    "BoldCore-Localizations": [
      "BoldCore/Localizations/*.lproj"
    ]
  },
  "preserve_paths": "BoldCore/*.{js,json}",
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  }
}
